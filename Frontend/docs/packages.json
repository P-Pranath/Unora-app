{
  "name": "cenomi new app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --active-arch-only",
    "android-build": "cd android && ./gradlew assembleRelease --stacktrace",
    "android-build-arm": "cd android && ./gradlew assembleRelease --stacktrace -PreactNativeArchitectures=arm64-v8a",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "start": "react-native start",
    "start-r": "react-native start --reset-cache",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "clean-android": "cd android && ./gradlew clean",
    "cb-android": "yarn install && yarn clean-android && yarn android",
    "ios-pod": "cd ios && rm -rf Pods && xcodebuild clean  && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "ios-clean-pod": "cd ios && rm -rf Pods && rm -rf Podfile.lock &&  xcodebuild clean  && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "pod-deep-remove": "rm -rf ~/Library/Caches/CocoaPods && rm -rf ios/Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd ios && rm -rf Podfile.lock && bundle exec pod deintegrate && bundle exec pod setup && xcodebuild clean",
    "clean-cache-android-ios-build": "watchman watch-del-all && rm -rf $TMPDIR/react-* $TMPDIR/metro-bundler-cache-*  $TMPDIR/react-native-packager-cache-* rm -rf node_modules/ ios/Podfile.lock ios/Pods ios/build android/app/build",
    "test": "jest",
    "postinstall": "patch-package",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "prepare": "husky"
  },
